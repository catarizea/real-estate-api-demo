{
  "name": "real-estate-api-demo",
  "version": "1.0.1",
  "description": "REST API demo for a search app to rent a property. Tech stack Bun, Hono, Drizzle, Planetscale.",
  "main": "index.ts",
  "scripts": {
    "db:push": "dotenv -e .env -- drizzle-kit push:mysql --config=drizzle.config.ts",
    "db:generate:migration": "dotenv -e .env -- drizzle-kit generate:mysql --config=drizzle.config.ts",
    "db:migrate": "dotenv -e .env -- bun ./src/utils/db/migrate.ts",
    "db:seed": "dotenv -e .env -- bun ./src/utils/db/seed/index.ts",
    "db:seed:addresses": "dotenv -e .env -- bun ./src/utils/db/seed/generateAddresses.ts",
    "db:seed:communities": "dotenv -e .env -- bun ./src/utils/db/seed/scrapeCommunities.ts",
    "db:seed:community": "dotenv -e .env -- bun ./src/utils/db/seed/scrapeCommunity.ts",
    "db:studio": "dotenv -e .env -- drizzle-kit studio --config=drizzle.config.ts --port=3001",
    "dev": "dotenv -e .env -- bun --hot index.ts",
    "debug": "dotenv -e .env -- bun --inspect index.ts",
    "start": "dotenv -e .env -v BUN_ENV=prod -- bun index.ts",
    "test": "dotenv -e .env -v BUN_ENV=test -- bun test"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.2.1",
    "@hono/zod-openapi": "^0.9.5",
    "@paralleldrive/cuid2": "^2.2.2",
    "@planetscale/database": "^1.13.0",
    "axios": "^1.6.5",
    "axios-retry": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "cloudinary": "^1.41.3",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "geolib": "^3.3.4",
    "hono": "^3.12.5",
    "mysql2": "^3.7.0",
    "node-geocoder": "^4.2.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/bun": "^1.0.0",
    "@types/node-geocoder": "^4.2.6",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "concurrently": "^8.2.2",
    "cron": "^3.1.6",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "path": "^0.12.7",
    "prettier": "^3.2.2"
  }
}
